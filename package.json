{
  "name": "bummar",
  "version": "1.1.0",
  "description": "npm cli generate test files",
  "main": "cli.js",
  "bin": {
    "bummar": "./cli.js"
  },
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "test",
    "structure",
    "generator"
  ],
  "author": "Matheus Genteluci <mgenteluci97@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "jest": "^24.7.1"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^yargs$": "<rootDir>/test/unit/mock/yargs.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": -10
      }
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
